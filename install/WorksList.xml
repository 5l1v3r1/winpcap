    <Cell><Data ss:Type="String">support for the &quot;Any&quot; device, in a way similar to linux</Data></Cell>
    <Cell ss:Index="9"><Data ss:Type="String">LOW</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="26.25">
    <Cell><Data ss:Type="Number">23</Data></Cell>
